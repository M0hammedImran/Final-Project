<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('Head'); -%>
  </head>
  <body>
    <%- include('Header'); -%>
    <div class="main-cont">
      <div class="contt">
        <div class="info">
          <h2>First Name</h2>
          <h3><%= user.firstName%></h3>
          <br />
          <h2>Last Name</h2>
          <h3><%= user.lastName%></h3>
          <br />
          <h2>Date of Birth</h2>
          <h3><%= user.DOB%></h3>
          <br />
          <h2>Address Line 1</h2>
          <h3><%= user.address1%></h3>
          <br />
          <h2>Address Line 2</h2>
          <h3><%= user.address2%></h3>
          <br />
          <h2>Email</h2>
          <h3><%= user.email%></h3>
          <br />
          <h2>Phone No.</h2>
          <h3><%= user.phone%></h3>
          <br />
        </div>
      </div>
      <form action="/dashboard/confirmuserremoval" method="POST">
        <div class="sub-btn">
          <button type="submit">Confirm</button>
        </div>
      </form>
    </div>
  </body>
</html>
